<h1>Listing academic_terms</h1>

<table>
  <tr>
    <th>Title</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @academic_terms.each do |academic_term| %>
  <tr>
    <td><%= academic_term.title %></td>
    <td><%= link_to 'Show', academic_term %></td>
    <td><%= link_to 'Edit', edit_academic_term_path(academic_term) %></td>
    <td><%= link_to 'Destroy', academic_term, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

