<%= render 'review' %>

<!-- Help Popover -->
<%
  @help = {
    :two => {title: '2',
      content: "Click \"Continue\" to next page.",
      position: 'right',
      style: ''},
  }
%>

<%= form_for @offering do |f| %>
  <%= redirect_token @redirect_url %>
  <%= f.hidden_field :review_done, value: true %>
  <%= f.submit class: 'btn btn-primary', value: 'Continue' %> &nbsp;
  <%= insert_help :two %>
<% end %>