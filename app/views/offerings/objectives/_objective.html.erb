<% fields_for_args ||= {} %>
<%= f.no_bs_fields_for :objectives, objective, fields_for_args do |of| %>
  <tr class="<%= 'cloneable' if objective.new_record?%>">
    <th>
      <%= of.text_field :description, placeholder: 'Objective Description' %>
      <%= of.hidden_field :_destroy, value: 1, disabled: true %>
      <%= of.hidden_field :position, :class => "position" %>
      <button class="btn btn-danger delete-mapping">
        <i class="icon-white icon-remove">&nbsp;</i>
      </button>
    </th>
    <%= of.fields_for :mappings do |mf| %>
      <td class="outcome">
        <%= mf.check_box :value %>
        <%= mf.hidden_field :outcome_id %>
      </td>
    <% end %>
  </tr>
<% end %>
